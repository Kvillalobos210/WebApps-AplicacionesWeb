A,EAAA;AACA,QAAA,EAAA,CAAA,MAAA,GAAA,QAAA,CAAA,IAAA;AACA,OAHA,EAGA,KAHA,CAGA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,OALA;AAMA,KATA;AAUA,IAAA,QAVA,oBAUA,IAVA,EAUA;AACA,WAAA,MAAA,GAAA,IAAA,CAAA,MAAA;AACA,WAAA,QAAA,GAAA,IAAA,CAAA,QAAA;AACA,WAAA,WAAA,GAAA,CAAA;AACA,WAAA,MAAA,GAAA,IAAA;AACA,KAfA;AAgBA,IAAA,UAhBA,sBAgBA,IAhBA,EAgBA;AACA,UAAA,EAAA,GAAA,IAAA;AACA,UAAA,OAAA,CAAA,iDAAA,CAAA,EACA,KAAA,CAAA,MAAA,CAAA,cAAA,IAAA,CAAA,MAAA,EAAA;AACA,kBAAA,IAAA,CAAA;AADA,OAAA,EAEA,IAFA,CAEA,UAAA,QAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,IAAA,CAAA,MAAA;AACA,QAAA,EAAA,CAAA,UAAA;AACA,QAAA,EAAA,CAAA,KAAA;AACA,QAAA,EAAA,CAAA,KAAA;AACA,OAPA,EAOA,KAPA,CAOA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,OATA;AAUA,KA7BA;AA8BA,IAAA,KA9BA,mBA8BA;AACA,WAAA,MAAA,GAAA,KAAA;AACA,WAAA,WAAA,GAAA,CAAA,CAAA;AACA,WAAA,KAAA;AACA,KAlCA;AAmCA,IAAA,KAnCA,mBAmCA;AACA,WAAA,MAAA,GAAA,EAAA;AACA,WAAA,QAAA,GAAA,EAAA;AACA,WAAA,WAAA,GAAA,CAAA,CAAA;AACA,KAvCA;AAwCA,IAAA,IAxCA,kBAwCA;AACA,UAAA,EAAA,GAAA,IAAA;;AACA,UAAA,KAAA,WAAA,GAAA,CAAA,CAAA,EAAA;AACA,QAAA,KAAA,CAAA,GAAA,CAAA,kBAAA,EAAA;AACA,oBAAA,EAAA,CAAA,MADA;AAEA,sBAAA,EAAA,CAAA;AAFA,SAAA,EAGA,IAHA,CAGA,UAAA,QAAA,EAAA;AACA,UAAA,EAAA,CAAA,KAAA;AACA,UAAA,EAAA,CAAA,UAAA;AACA,UAAA,EAAA,CAAA,KAAA;AACA,SAPA,EAOA,KAPA,CAOA,UAAA,KAAA,EAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,SATA,EADA,CAWA;AACA,OAZA,MAaA;AACA,QAAA,KAAA,CAAA,IAAA,CAAA,UAAA,EAAA;AACA,sBAAA,EAAA,CAAA;AADA,SAAA,EAEA,IAFA,CAEA,UAAA,QAAA,EAAA;AACA,UAAA,EAAA,CAAA,KAAA;AACA,UAAA,EAAA,CAAA,UAAA;AACA,UAAA,EAAA,CAAA,KAAA;AACA,SANA,EAMA,KANA,CAMA,UAAA,KAAA,EAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,SARA;AASA;;AACA,WAAA,KAAA;AACA;AAnEA;AA1BA,CAAA","sourcesContent":["<template>\r\n  <v-container fluid>\r\n    <v-data-iterator\r\n      :items=\"cities\"\r\n      item-key=\"name\"\r\n      :items-per-page=\"100\"\r\n      hide-default-footer\r\n    >\r\n        <template v-slot:header>\r\n        <v-toolbar\r\n          class=\"mb-2\"\r\n          color=\"indigo darken-5\"\r\n          dark\r\n          flat\r\n        >\r\n\r\n          <v-toolbar-title>Lista de departamentos disponibles del Perú</v-toolbar-title>\r\n        </v-toolbar>\r\n        <v-dialog v-model=\"dialog\" max-width=\"800px\">\r\n                    <template v-slot:activator=\"{ on }\">\r\n                        <v-btn color=\"primary\" dark class=\"mb-2\" v-on=\"on\">Nuevo Departamento</v-btn>\r\n                    </template>\r\n                    <v-card>\r\n                        <v-card-title>\r\n                            <span class=\"headline\">{{ formTitle }}</span>\r\n                        </v-card-title>\r\n\r\n                        <v-card-text>\r\n                            <v-container>\r\n                                <v-row>\r\n                                    <v-col cols=\"12\" sm=\"12\" md=\"12\">\r\n                                        <v-text-field v-model=\"cityname\" label=\"Nombre del departamento\"></v-text-field>\r\n                                    </v-col>\r\n                                </v-row>\r\n                            </v-container>\r\n                            <v-card-actions>\r\n                            <v-spacer></v-spacer>\r\n                            <v-btn color=\"blue darken-1\" text @click=\"close\">Cancel</v-btn>\r\n                            <v-btn color=\"blue darken-1\" text @click=\"save\">Save</v-btn>\r\n                        </v-card-actions>\r\n\r\n                        </v-card-text>\r\n                    </v-card>\r\n        </v-dialog>\r\n        <!--<v-btn color=\"primary\" dark class=\"mb-2\" v-on=\"on\">New City</v-btn>-->\r\n      </template>\r\n      <template v-slot:default=\"{ items }\" >\r\n        <v-row>\r\n          <v-col\r\n            v-for=\"item in items\"\r\n            :key=\"item.name\"\r\n            cols=\"12\"\r\n            sm=\"6\"\r\n            md=\"4\"\r\n            lg=\"3\"\r\n          >\r\n            <v-card>\r\n              <v-card-title>\r\n                <h4>{{ item.cityName }}</h4>\r\n            <v-btn\r\n              color=\"accent\"\r\n              fab\r\n              small\r\n              dark\r\n              class=\"mx-16\"\r\n            >\r\n              <td>\r\n                    <v-icon @click=\"editItem(item)\">edit</v-icon>\r\n              </td>\r\n              </v-btn>\r\n            </v-card-title>\r\n            <v-divider></v-divider>\r\n              <td class=\"justify-center layout px-0\">\r\n                <h4>id: {{ item.cityId }}</h4>\r\n              </td>\r\n              \r\n            </v-card>\r\n          </v-col>\r\n        </v-row>\r\n      </template>\r\n    </v-data-iterator>\r\n  </v-container>\r\n</template>\r\n\r\n<script>\r\n\r\n    import axios from 'axios'\r\n    \r\n    export default {\r\n        data:() => ({\r\n            search: '',\r\n            cityid: '',\r\n            cityname:'',\r\n            dialog: false,\r\n            editedIndex: -1,\r\n            cities: [],\r\n            headers: [\r\n                { text: 'CityName', value: 'cityname', sortable: true },\r\n                \r\n            ]\r\n        }),\r\n        computed: {\r\n            formTitle() {\r\n                return this.editedIndex === -1 ? 'Agregar Departamento' : 'Editar Departamento';\r\n            },\r\n        },\r\n        watch: {\r\n            dialog (val) {\r\n                val || this.close()\r\n            },\r\n        },\r\n        created() {\r\n            this.listCities();\r\n        },\r\n        methods: {\r\n            listCities() {\r\n                let me = this;\r\n                axios.get('api/City')\r\n                .then(function(response){\r\n                    me.cities = response.data;\r\n                }).catch(function(error){\r\n                    console.log(error);\r\n                })\r\n            },\r\n            editItem(item) {\r\n                this.cityid = item.cityId;\r\n                this.cityname = item.cityName;\r\n                this.editedIndex = 1;\r\n                this.dialog = true;\r\n            },\r\n            deleteItem(item) {\r\n                let me = this;\r\n                if(confirm('¿Estás seguro que quieres eliminar esta ciudad?'))\r\n                    axios.delete('api/City/' + item.cityId, {\r\n                        'cityid': item.cityId\r\n                    }).then(function(response){\r\n                        console.log(item.CityId);\r\n                        me.listCities();\r\n                        me.close();\r\n                        me.clean();\r\n                    }).catch(function(error){\r\n                        console.log(error);\r\n                    })\r\n            },\r\n            close() {\r\n                this.dialog = false;\r\n                this.editedIndex=-1;\r\n                this.clean();\r\n            },\r\n            clean() {\r\n                this.cityid=\"\";\r\n                this.cityname=\"\";\r\n                this.editedIndex=-1;\r\n            },\r\n            save() {\r\n                let me = this;\r\n                if(this.editedIndex > -1) {\r\n                    axios.put('api/City/PutCity',{\r\n                        'cityid': me.cityid,\r\n                        'cityname': me.cityname\r\n                    }).then(function(response){\r\n                        me.close();\r\n                        me.listCities();\r\n                        me.clean();\r\n                    }).catch(function(error) {\r\n                        console.log(error);\r\n                    });\r\n                    //me.clean();\r\n                }\r\n                else {\r\n                    axios.post('api/City', {\r\n                        'cityname': me.cityname\r\n                    }).then(function(response){\r\n                        me.close();\r\n                        me.listCities();\r\n                        me.clean();\r\n                    }).catch(function(error) {\r\n                        console.log(error);\r\n                    });\r\n                }\r\n                this.close();\r\n            },\r\n        },\r\n    }\r\n</script>\r\n"],"sourceRoot":"src/components"}]}