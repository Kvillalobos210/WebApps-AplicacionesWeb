aADA,2BACA;AACA,UAAA,EAAA,GAAA,IAAA;AACA,MAAA,KAAA,CAAA,GAAA,CAAA,cAAA,EAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,QAAA,EAAA,CAAA,SAAA,GAAA,QAAA,CAAA,IAAA;AACA,OAFA,EAEA,KAFA,CAEA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,OAJA;AAKA,KARA;AASA,IAAA,QATA,oBASA,IATA,EASA;AACA,WAAA,UAAA,GAAA,IAAA,CAAA,UAAA;AACA,WAAA,YAAA,GAAA,IAAA,CAAA,YAAA;AACA,WAAA,MAAA,GAAA,IAAA,CAAA,MAAA;AACA,WAAA,WAAA,GAAA,CAAA;AACA,WAAA,MAAA,GAAA,IAAA;AACA,KAfA;AAgBA,IAAA,UAhBA,sBAgBA,IAhBA,EAgBA;AACA,UAAA,EAAA,GAAA,IAAA;AACA,UAAA,OAAA,CAAA,mDAAA,CAAA,EACA,KAAA,CAAA,MAAA,CAAA,kBAAA,IAAA,CAAA,UAAA,EAAA;AACA,sBAAA,IAAA,CAAA;AADA,OAAA,EAEA,IAFA,CAEA,UAAA,QAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,IAAA,CAAA,UAAA;AACA,QAAA,EAAA,CAAA,aAAA;AACA,QAAA,EAAA,CAAA,KAAA;AACA,QAAA,EAAA,CAAA,KAAA;AACA,OAPA,EAOA,KAPA,CAOA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,OATA;AAUA,KA7BA;AA8BA,IAAA,KA9BA,mBA8BA;AACA,WAAA,MAAA,GAAA,KAAA;AACA,WAAA,KAAA;AACA,KAjCA;AAkCA,IAAA,KAlCA,mBAkCA;AACA,WAAA,UAAA,GAAA,EAAA;AACA,WAAA,YAAA,GAAA,EAAA;AACA,WAAA,MAAA,GAAA,EAAA;AACA,WAAA,WAAA,GAAA,CAAA,CAAA;AACA,KAvCA;AAwCA,IAAA,IAxCA,kBAwCA;AACA,UAAA,EAAA,GAAA,IAAA;;AACA,UAAA,KAAA,WAAA,GAAA,CAAA,CAAA,EAAA;AACA,QAAA,KAAA,CAAA,GAAA,CAAA,0BAAA,EAAA;AACA,wBAAA,EAAA,CAAA,UADA;AAEA,0BAAA,EAAA,CAAA,YAFA;AAGA,oBAAA,EAAA,CAAA;AAHA,SAAA,EAKA,IALA,CAKA,UAAA,QAAA,EAAA;AACA,UAAA,EAAA,CAAA,KAAA;AACA,UAAA,EAAA,CAAA,aAAA;AACA,UAAA,EAAA,CAAA,KAAA;AACA,SATA,EASA,KATA,CASA,UAAA,KAAA,EAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,SAXA;AAYA,OAbA,MAcA;AACA,QAAA,KAAA,CAAA,IAAA,CAAA,cAAA,EAAA;AACA,0BAAA,EAAA,CAAA,YADA;AAEA,oBAAA,EAAA,CAAA;AAFA,SAAA,EAGA,IAHA,CAGA,UAAA,QAAA,EAAA;AACA,UAAA,EAAA,CAAA,KAAA;AACA,UAAA,EAAA,CAAA,aAAA;AACA,UAAA,EAAA,CAAA,KAAA;AACA,SAPA,EAOA,KAPA,CAOA,UAAA,KAAA,EAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,SATA;AAUA;;AACA,WAAA,KAAA;AACA;AArEA;AA9BA,CAAA","sourcesContent":["<template>\r\n  <v-container fluid>\r\n    <v-data-iterator\r\n      :items=\"districts\"\r\n      item-key=\"name\"\r\n      :items-per-page=\"100\"\r\n      hide-default-footer\r\n    >\r\n        <template v-slot:header>\r\n        <v-toolbar\r\n          class=\"mb-2\"\r\n          color=\"indigo darken-5\"\r\n          text @click=\"cities\"\r\n          dark\r\n          flat\r\n        >\r\n        <v-toolbar-title>Lista de distritos disponibles del Perú</v-toolbar-title>\r\n        </v-toolbar>\r\n        <v-dialog v-model=\"dialog\" max-width=\"800px\">\r\n                    <template v-slot:activator=\"{ on }\">\r\n                        <v-btn color=\"primary\" dark class=\"mb-2\" v-on=\"on\">Nuevo Distrito</v-btn>\r\n                    </template>\r\n                    <v-card>\r\n                        <v-card-title>\r\n                            <span class=\"headline\">{{ formTitle }}</span>\r\n                        </v-card-title>\r\n\r\n                        <v-card-text>\r\n                            <v-container>\r\n                                <v-row>\r\n                                    <v-col cols=\"12\" sm=\"12\" md=\"12\">\r\n                                        <v-text-field v-model=\"districtname\" label=\"Nombre del distrito\"></v-text-field>\r\n                                    </v-col>\r\n                                    <v-col cols=\"12\" sm=\"12\" md=\"12\">\r\n                                        <v-text-field v-model=\"cityid\" label=\"id del Departamento\"></v-text-field>\r\n                                    </v-col>\r\n                                </v-row>\r\n                            </v-container>\r\n                            <v-card-actions>\r\n                            <v-spacer></v-spacer>\r\n                            <v-btn color=\"blue darken-1\" text @click=\"close\">Cancel</v-btn>\r\n                            <v-btn color=\"blue darken-1\" text @click=\"save\">Save</v-btn>\r\n                        </v-card-actions>\r\n\r\n                        </v-card-text>\r\n                    </v-card>\r\n        </v-dialog>\r\n        <!--<v-btn color=\"primary\" dark class=\"mb-2\" v-on=\"on\">New City</v-btn>-->\r\n      </template>\r\n        <template v-slot:default=\"{ items }\">\r\n        <v-row>\r\n          <v-col\r\n            v-for=\"item in items\"\r\n            :key=\"item.name\"\r\n            cols=\"12\"\r\n            sm=\"6\"\r\n            md=\"4\"\r\n            lg=\"3\"\r\n          >\r\n            <v-card>\r\n              <v-card-title>\r\n                <h4>{{ item.districtName }}</h4>\r\n                <v-btn\r\n              color=\"accent\"\r\n              fab\r\n              small\r\n              dark\r\n              class=\"mx-16\"\r\n            >\r\n              <td>\r\n                    <v-icon @click=\"editItem(item)\">edit</v-icon>\r\n              </td></v-btn>\r\n              </v-card-title>\r\n              <v-divider></v-divider>\r\n              <td class=\"justify-center layout px-0\">\r\n                <h4>id: {{ item.districtId }}</h4>\r\n              </td>\r\n              <v-divider></v-divider>\r\n              <td class=\"justify-center layout px-0\">\r\n                <h4>id de Departamento: {{ item.cityId }}</h4>\r\n              </td>\r\n            </v-card>\r\n          </v-col>\r\n        </v-row>\r\n\r\n      </template>\r\n    </v-data-iterator>\r\n  </v-container>\r\n</template>\r\n\r\n\r\n\r\n<script>\r\n    import axios from 'axios'\r\n    \r\n    export default {\r\n        data:() => ({\r\n            search: '',\r\n            districtid: '',\r\n            districtname:'',\r\n            cityid: '',\r\n            cityname: '',\r\n            dialog: false,\r\n            editedIndex: -1,\r\n            districts: [],\r\n            cities: [],\r\n            items: [\r\n                { text: 'CityName', value: 'cityname', sortable: true }\r\n            ]\r\n            \r\n        }),\r\n        computed: {\r\n            formTitle() {\r\n                return this.editedIndex === -1 ? 'Agregar Distrito': 'Editar Distrito';\r\n            }\r\n        },\r\n        watch: {\r\n            dialog (val) {\r\n                val || this.close()\r\n            }\r\n        },\r\n        created() {\r\n            this.listDistricts();\r\n        },\r\n\r\n        methods: {\r\n            listDistricts() {\r\n                let me= this;\r\n                axios.get('api/District').then(function(response){\r\n                    me.districts = response.data;\r\n                }).catch(function(error){\r\n                    console.log(error);\r\n                })\r\n            },\r\n            editItem(item) {\r\n                this.districtid = item.districtId;\r\n                this.districtname = item.districtName;\r\n                this.cityid = item.cityId;\r\n                this.editedIndex = 1;\r\n                this.dialog = true;\r\n            },\r\n            deleteItem(item) {\r\n                let me = this;\r\n                if(confirm('¿Estás seguro que quieres eliminar este distrito?'))\r\n                    axios.delete('api/District/' + item.districtId, {\r\n                        'districtid': item.districtId\r\n                    }).then(function(response){\r\n                        console.log(item.DistrictId);\r\n                        me.listDistricts();\r\n                        me.close();\r\n                        me.clean();\r\n                    }).catch(function(error){\r\n                        console.log(error);\r\n                    })\r\n            },\r\n            close() {\r\n                this.dialog = false;\r\n                this.clean();\r\n            },\r\n            clean() {\r\n                this.districtid = \"\";\r\n                this.districtname = \"\";\r\n                this.cityid = \"\";\r\n                this.editedIndex=-1;\r\n            },\r\n            save() {\r\n                let me=this;\r\n                if(this.editedIndex >- 1) {\r\n                    axios.put('api/District/PutDistrict',{\r\n                        'districtid': me.districtid,\r\n                        'districtname': me.districtname,\r\n                        'cityid': me.cityid\r\n\r\n                    }).then(function(response){\r\n                        me.close();\r\n                        me.listDistricts();\r\n                        me.clean();\r\n                    }).catch(function(error) {\r\n                        console.log(error);\r\n                    });\r\n                }\r\n                else {\r\n                    axios.post('api/District', {\r\n                        'districtname': me.districtname,\r\n                        'cityid': me.cityid\r\n                    }).then(function(response){\r\n                        me.close();\r\n                        me.listDistricts();\r\n                        me.clean();\r\n                    }).catch(function(error) {\r\n                        console.log(error);\r\n                    });\r\n                }\r\n                this.close();\r\n            }\r\n        }\r\n    }\r\n</script>"],"sourceRoot":"src/components"}]}